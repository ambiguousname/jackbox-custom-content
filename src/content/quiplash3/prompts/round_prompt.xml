<?xml version="1.0" encoding="UTF-8"?>
<!--The name of the window. The window should have an ::ensure_all_types() function.-->
<window name="QuiplashRoundPrompt">
	<!--Where to store all of the content in the game folder as well as the mods folder:-->
	<game-folder>Quiplash3</game-folder>
	<content-list>
		<content type="Round1">
			<subcontent-list>
				<subcontent type="Manifest">Quiplash3Round1Question.jet</subcontent>
			</subcontent-list>
		</content>
		<content type="Round2">
			<subcontent-list>
				<subcontent type="Manifest">Quiplash3Round2Question.jet</subcontent>
			</subcontent-list>
		</content>
		<content type="FinalRound">
			<subcontent-list>
				<subcontent type="Manifest">Quiplash3FinalQuestion.jet</subcontent>
			</subcontent-list>
		</content>
	</content-list>
</window>