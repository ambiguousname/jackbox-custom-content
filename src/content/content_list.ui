<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<!-- Loaded in templates/mainmenu/content_creation/game_list.rs -->
	<object class="GListStore" id="content_list">
		<property name="item-type">JCCGameListItem</property>
		<child>
			<object class="JCCGameListItem">
				<property name="title">Jackbox Party Pack 7</property>
				<property name="children">
					<object class="GListStore">
						<property name="item-type">JCCGameListItem</property>
						<child>
							<object class="JCCGameListItem">
								<property name="title">Quiplash 3</property>
								<property name="content">
									<object class="GListStore">
										<property name="item-type">JCCContent</property>
										<child>
											<object class="JCCContent">
												<property name="title">Round Prompt</property>
												<!-- build/content_list.rs uses a regex search to find these two and ensure correct typing so the build system works.
												Don't add any properties between these two, otherwise the Regex won't work.

												window-path is the path to the window's .rs file that implement ContentWindow.
												window is just the object (for the list store to read).
												-->
												<property name="window-path">quiplash3::prompts</property>
												<property name="window">
													<object class="JCCQuiplashRoundPrompt"></object>
												</property>
											</object>
										</child>
									</object>
								</property>
							</object>
						</child>
					</object>
				</property>
			</object>
		</child>
	</object>
</interface>